{
  "directories": {
    "javascript": {
      "input": "main.js",
      "output": {
        "dir": "./public/js",
        "fileName": "app.js",
        "publicPath": "/js/"
      }
    },
    "stylus": {
      "input": "vivid-finance.styl",
      "output": "public/css/app.css"
    },
    "images": {
      "input": "resources/assets/img",
      "output": "public/img"
    },
    "metadata": {
      "input": "resources/assets/metadata",
      "output": "public/metadata"
    },
    "fonts": {
      "input": [
        "resources/assets/fonts"
      ],
      "output": "public/fonts"
    }
  },
  "version": [
    "css/app.css",
    "js/app.js",
    "metadata/favicon/favicon.ico",
    "metadata/favicon/favicon-16x16.png",
    "metadata/favicon/favicon-32x32.png",
    "metadata/favicon/favicon-96x96.png",
    "metadata/favicon/favicon-194x194.png",
    "metadata/android/android-chrome-36x36.png",
    "metadata/android/android-chrome-48x48.png",
    "metadata/android/android-chrome-72x72.png",
    "metadata/android/android-chrome-96x96.png",
    "metadata/android/android-chrome-144x144.png",
    "metadata/android/android-chrome-192x192.png",
    "metadata/android/manifest.json",
    "metadata/apple/apple-touch-icon.png",
    "metadata/apple/apple-touch-icon-57x57.png",
    "metadata/apple/apple-touch-icon-60x60.png",
    "metadata/apple/apple-touch-icon-72x72.png",
    "metadata/apple/apple-touch-icon-76x76.png",
    "metadata/apple/apple-touch-icon-114x114.png",
    "metadata/apple/apple-touch-icon-120x120.png",
    "metadata/apple/apple-touch-icon-144x144.png",
    "metadata/apple/apple-touch-icon-152x152.png",
    "metadata/apple/apple-touch-icon-180x180.png",
    "metadata/apple/apple-touch-icon-precomposed.png",
    "metadata/microsoft/mstile-70x70.png",
    "metadata/microsoft/mstile-144x144.png",
    "metadata/microsoft/mstile-150x150.png",
    "metadata/microsoft/mstile-310x150.png",
    "metadata/microsoft/mstile-310x310.png",
    "metadata/microsoft/browserconfig.xml",
    "metadata/safari/safari-pinned-tab.svg",
    "img/logo/logo.svg"
  ],
  "phpUnit": {
    "src": "./tests/",
    "command": "./vendor/bin/phpunit"
  },
  "browserSync": {
    "proxy": "vivid-finance.app"
  }
}